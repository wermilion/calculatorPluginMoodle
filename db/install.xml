<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/calculator/db"
       VERSION="20240412035"
       COMMENT="XMLDB для плагина 'Калькулятор'"
>
    <TABLES>
        <TABLE NAME="calculator" COMMENT="Таблица, хранящяя результаты вычисления квадратного уравнения">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="a" TYPE="float" NOTNULL="true"/>
                <FIELD NAME="b" TYPE="float" NOTNULL="true"/>
                <FIELD NAME="c" TYPE="float" NOTNULL="true"/>
                <FIELD NAME="x1" TYPE="float" NOTNULL="false"/>
                <FIELD NAME="x2" TYPE="float" NOTNULL="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>